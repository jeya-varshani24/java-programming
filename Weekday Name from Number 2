import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        String day;
        int n = in.nextInt();
        switch(n){
            case 1:
               day = "Monday";
               System.out.println(day);
               break;
            case 2:
               day = "Tuesday"; 
               System.out.println(day);
               break;
            case 3:
               day = "Wednesday";
               System.out.println(day);
               break;
            case 4:
               day = "Thursday";
               System.out.println(day);
               break;
            case 5:
               day = "Friday"; 
               System.out.println(day);
               break;
            case 6:
               day = "Saturday"; 
               System.out.println(day);
               break;
            case 7:
               day = "Sunday";
               System.out.println(day);
               break;
            default:
                System.out.println("Enter a valid Input");         
        }

    }
}
