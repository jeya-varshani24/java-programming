import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        int s = (a/10)+(a%10);
        System.out.println("Bird said:"+a);
        System.out.println("Alice must go in path-"+s);
    }
}
